# Configuración de Supabase
# Copia este archivo a .env.local y completa con tus credenciales

# URL de tu proyecto Supabase (Settings > API > Project URL)
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co

# Anon/Public Key de Supabase (Settings > API > Project API keys > anon public)
VITE_SUPABASE_ANON_KEY=tu_anon_key_aqui

# Configuración de la aplicación
VITE_APP_NAME=CuidoAMiTata
VITE_AUTO_LOGOUT_MINUTES=15
